#!/bin/bash

# Program to output a system info page.

TITLE="System Information Report For $HOSTNAME"
CURRENT_TIME="$(date +"%x %r %Z")"
TIMESTAMP="Generated $CURRENT_TIME, by $USER"

cat << _EOF_
<html>
 	<head>
 		<TITLE>$TITLE</TITLE>
 	</head>
 	<body>
 		<h1>$TITLE</h1>
		<p>$TIMESTAMP</p>
 	</body>
</html>
_EOF_
